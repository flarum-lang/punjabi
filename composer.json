{
	"name": "flarum-lang/punjabi",
	"type": "flarum-extension",
	"description": "Punjabi language pack for Flarum.",
	"homepage": "https://github.com/flarum-lang/punjabi",
	"keywords": [
		"locale",
		"flarum",
		"flarum-locale",
		"pa-locale",
		"punjabi-locale",
		"punjabi"
	],
	"license": "MIT",
	"support": {
		"issues": "https://github.com/flarum-lang/punjabi/issues",
		"source": "https://github.com/flarum-lang/punjabi"
	},
	"require": {
		"flarum/core": "^1.6"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"extra": {
		"flarum-extension": {
			"title": "Punjabi",
			"category": "language",
			"icon": {
				"image": "icon.svg",
				"backgroundSize": "cover",
				"backgroundPosition": "center"
			}
		},
		"flarum-locale": {
			"code": "pa",
			"title": "Punjabi"
		},
		"extiverse": {
			"discuss": ""
		}
	}
}
